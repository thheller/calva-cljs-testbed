{
  "name": "calvacljstestbed",
  "displayName": "CalvaCLJSTestbed",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "calvacljstestbed.helloWorld"
  ],
  "main": "./lib/ext.js",
  "contributes": {
    "commands": [
      {
        "command": "calvacljstestbed.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "calvacljstestbed.helloCljsLib",
        "title": "Hello cljs-lib"
      },
      {
        "command": "calvacljstestbed.helloTypeScript",
        "title": "Hello TypeScript"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile-cljs": "npx shadow-cljs watch ext",
    "release-cljs": "npx shadow-cljs release ext",
    "watch-ts": "npx tsc --watch --project ./tsconfig.json",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "clean": "rimraf ./out ./tsconfig.tsbuildinfo ./cljs-out ./lib ./src/cljs-lib/out",
    "bundle": "esbuild public/js/main.js --bundle --platform=node --packages=external --outfile=public/js/main.bundle.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^9.1.0",
    "@types/node": "^18.6.3",
    "@types/vscode": "^1.67.0",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@vscode/test-electron": "^2.2.2",
    "esbuild": "0.17.10",
    "eslint": "^8.8.0",
    "glob": "^7.1.6",
    "mocha": "^9.2.2",
    "rimraf": "^2.7.1",
    "shadow-cljs": "^2.20.20",
    "typescript": "^4.6.3"
  }
}
